        function showPricelist(platform) {
            const modal = document.getElementById('pricelistModal');
            const title = document.getElementById('modalTitle');
            const content = document.getElementById('pricelistContent');
            
            title.textContent = pricelistData[platform].title;
            content.innerHTML = pricelistData[platform].content;
            
            modal.classList.remove('hidden');
            document.body.style.overflow = 'hidden';
        }

        function closePricelist() {
            const modal = document.getElementById('pricelistModal');
            modal.classList.add('hidden');
            document.body.style.overflow = 'auto';
        }

        // Close modal when clicking outside
        document.getElementById('pricelistModal').addEventListener('click', function(e) {
            if (e.target === this) {
                closePricelist();
            }
        });
    </script>
